{
  "formatVersion": 1,
  "plugins": ["vcfview"],
  "tracks": [
    {
      "category": "Reference sequence",
      "chunkSize": 20000,
      "key": "Reference sequence",
      "label": "DNA",
      "seqType": "dna",
      "storeClass": "JBrowse/Store/Sequence/StaticChunked",
      "type": "SequenceTrack",
      "urlTemplate": "seq/refSeqs.json"
    },

    {
      "urlTemplate": "inputVcfs/HepG2.vcf.gz",
      "storeClass": "vcfview/Store/SeqFeature/segmentation_complete",
      "type": "MultiBigWig/View/Track/MultiWiggle/MultiXYPlot",
      "label": "HepG2 Multiline with GC(complete)",
      "chunkSizeLimit": 100000000,
      "autoscale": "local",
      "noFill": true,
      "gcContent": "hg19.100000.gc",
      "urlTemplates": [
        { "name": "HepG2", "color": "grey" },
        { "name": "HepG2_GC", "color": "black" },
        { "name": "HepG2_meanshift", "color": "red" },
        { "name": "HepG2_call", "color": "green" }
      ]
    },
    {
      "urlTemplate": "inputVcfs/HepG2.vcf.gz",
      "storeClass": "vcfview/Store/SeqFeature/BAFview",
      "type1": "JBrowse/View/Track/Wiggle/XYPlot",
      "type": "MultiBigWig/View/Track/MultiWiggle/MultiXYPlot",
      "label": "BAF view",
      "chunkSizeLimit": 100000000,
      "max_score": 1,
      "noFill": true,
      "GenotypeField": "AD",
      "urlTemplates": [
        { "name": "HepG2", "color": "red", "nonCont": true },
        { "name": "HepG2_BAF", "color": "blue", "nonCont": true},
        { "name": "HepG2_MIN", "color": "red", "nonCont": true}
      ]
    }
  ]
}
