{
  "formatVersion": 1,
  "plugins": ["vcfview"],
  "tracks": [
    {
      "category": "Reference sequence",
      "chunkSize": 20000,
      "key": "Reference sequence",
      "label": "DNA",
      "seqType": "dna",
      "storeClass": "JBrowse/Store/Sequence/StaticChunked",
      "type": "SequenceTrack",
      "urlTemplate": "seq/refSeqs.json"
    },
    {
      "storeClass": "vcfview/Store/SeqFeature/vcfview",
      "key": "Example type 1",
      "label": "test",
      "urlTemplate": "inputVcfs/trio.vcf.gz",
      "type1": "JBrowse/View/Track/HTMLVariants",
      "type": "JBrowse/View/Track/Wiggle/XYPlot",
      "autoscale": "local"
    },
    {
      "storeClass": "vcfview/Store/SeqFeature/SegmentationMultiValue",
      "key": "Example type 2",
      "label": "test2",
      "urlTemplate": "inputVcfs/trio.vcf.gz",
      "type": "JBrowse/View/Track/Wiggle/XYPlot",
      "autoscale": "local"
    },
    {
      "storeClass": "JBrowse/Store/SeqFeature/VCFTabix",
      "showTooltips": true,
      "showLabels": true,
      "clickTooltips": true,
      "key": "Segmentation Advanced",
      "label": "LGG 3",
      "type": "vcfview/View/Track/SegmentationAutoTrack",
      "urlTemplate": "inputVcfs/trio.vcf.gz",
      "noFill": true,
      "dpField": ["DP"],
      "adField": ["AD"],
      "autoscale": "local",
      "binSize": 100000
    },
    {
      "storeClass": "vcfview/Store/SeqFeature/SegmentationMultiValue",
      "urlTemplate": "inputVcfs/trio.vcf.gz",
      "showTooltips": true,
      "showLabels": true,
      "clickTooltips": true,
      "key": "Segmentation MultiBigWig",
      "label": "LGG 4",
      "type": "vcfview/View/Track/SegmentationAutoMultiTrack",
      "urlTemplates": [
        { "name": "main", "color": "green" },
        { "name": "secondary", "color": "purple" }
      ],
      "max_score": 100,
      "noFill": true,
      "binSize": 1000000,
      "dpField": ["DP"],
      "adField": ["AD"],
      "autoscale": "local",
      "chunkSizeLimit": 100000000
    },
    {
      "urlTemplate": "inputVcfs/trio.vcf.gz",
      "label": "trio vcf"
    },
    {
      "urlTemplate": "inputVcfs/trio.vcf.gz",
      "storeClass": "vcfview/Store/SeqFeature/VCFRawTabix",
      "type": "JBrowse/View/Track/Wiggle/XYPlot",
      "label": "trio vcf raw DP parser",
      "chunkSizeLimit": 100000000,
      "max_score": 100
    },
    {
      "urlTemplate": "inputVcfs/trio.vcf.gz",
      "storeClass": "vcfview/Store/SeqFeature/VCFExtra",
      "type": "JBrowse/View/Track/Wiggle/XYPlot",
      "label": "trio vcf normal DP parser",
      "chunkSizeLimit": 100000000,
      "max_score": 100
    },
    {
      "urlTemplate": "inputVcfs/trio.vcf.gz",
      "storeClass": "vcfview/Store/SeqFeature/VCFRawTabix",
      "type": "MultiBigWig/View/Track/MultiWiggle/MultiXYPlot",
      "label": "trio vcf multibigwig",
      "chunkSizeLimit": 100000000,
      "max_score": 5,
      "min_score": -5,
      "urlTemplates": [
        { "name": "NA12877", "color": "green" },
        { "name": "NA12878", "color": "orange" },
        { "name": "NA12882", "color": "blue" }
      ]
    },
    {
      "urlTemplate": "inputVcfs/trio.vcf.gz",
      "storeClass": "vcfview/Store/SeqFeature/VCFRawTabix",
      "type": "vcfview/View/Track/MultiVCF",
      "label": "trio vcf multivcf",
      "chunkSizeLimit": 100000000,
      "max_score": 100,
      "urlTemplates": [
        { "name": "NA12877", "color": "green" },
        { "name": "average", "color": "black" }
      ]
    }
  ]
}
